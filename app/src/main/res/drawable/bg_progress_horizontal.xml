<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="#673AB7"
                android:centerColor="#5E35B1"
                android:centerY="0.75"
                android:endColor="#5E35B1"
                android:angle="270"
                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#BA68C8"
                    android:centerColor="#AB47BC"
                    android:centerY="0.75"
                    android:endColor="#6A1B9A"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient

                    android:startColor="#F48FB1"
                    android:centerColor="#F8BBD0"
                    android:centerY="0.75"
                    android:endColor="#FCE4EC"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

</layer-list>